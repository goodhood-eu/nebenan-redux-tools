  ],
  "module": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "lib/types",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && npm run lint:typescript && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build": "NODE_ENV=production rollup -c",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "lint:typescript": "tsc --noEmit",
    "test": "TZ='GMT' mocha",
    "test:tofile": "TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/plugin-transform-strict-mode": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/chai": "^4.3.4",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.13",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browserslist-config-nebenan": "^1.0.1",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "eslint-config-nebenan": "^4.4.0",
    "eslint-config-nebenan-base": "^2.0.1",
    "mocha": "^10.2.0",
    "rollup": "^3.7.5",
    "rollup-plugin-typescript2": "^0.34.1",
    "sinon": "^15.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "escape-string-regexp-browser": "^1.1.0",
    "js-cookie": "^3.0.5",
    "local-storage-adapter": "^2.0.0",
